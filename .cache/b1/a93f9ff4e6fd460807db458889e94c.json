 with capitalization in search terms and search content\n    console.log(lc.includes(filter))\n    return lc.includes(filter)\n  });\n} else {\n  // If the search bar is empty, set newList to original task list\n  newList = this.props.items\n  \n}\n// Set the filtered state based on what our rules added to newList\nconsole.log(newList)\nthis.setState({\n  filtered: newList\n})\nconsole.log(this.state.filtered)\n\n}\n\n    render() {\n        return (\n            <div>\n                    <input type=\"text\" className=\"input\" onChange={this.handleChange} placeholder=\"Search...\" />     \n\n              <ul>\n                    {this.props.items.map(item =>(\n                        <li key={item}>{item} &nbsp;\n                            <span className=\"delete\" onClick={() => this.props.removeItem(item)} />\n                        </li>\n                    ))}\n               </ul>\n               </div>\n        \n        )\n    }\n}\n\nexport default List;"},"lineCount":null}},"error":null,"hash":"2ffff08682ed6e0615413fcd58aeffd2","cacheData":{"env":{}}}